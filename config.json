{
  "paths": {
    "build": "_dist/",
    "buildAssets": "_dist/assets/",
    "scss": "src/scss/",
    "files": "src/files/",
    "fonts": "src/fonts/",
    "icons": "src/icons/",
    "img": "src/img/",
    "js": "src/js/",
    "svgicon": "_svgicon",
    "pages": "src/pages"
  },
  "autoprefixer": {
    "browsers": ["last 2 versions", "IE >= 10"]
  },
  "metalsmith": {
    "plugins": {
      "metalsmith-in-place": {
        "engine": "handlebars",
        "partials": "./src/partials"
      }
    }
  }
}
